<div class="contact_wrapper">
<%= form_for(@contact, url: confirm_path) do |f| %>
  <div class="form-group">
    <%= f.label :name, 'お名前',class:"required" %>
    <%= f.text_field :name, autofocus: true, class: 'form-control',autocomplete:"off" %>
  </div>
  <div class="form-group">
    <%= f.label :email, 'メールアドレス', class:"required" %>
    <%= f.text_field :email, autofocus: true, class: 'form-control',autocomplete:"off" %>
  </div>
  <div class="form-group">
    <%= f.label :message, 'メッセージ', class:"required" %>
    <%= f.text_area :message, size: '10x5', class: 'form-control',autocomplete:"off" %>
  </div>
  <div class="form-group">
    <%= f.submit '入力内容確認',class: "myButton" %>
  </div>
<% end %>
</div>
